[run]
source = sorl_thumbnail_serializer
omit =
    */migrations/*
    */south_migrations/*
    tests
    sorl_thumbnail_serializer/__init__.py
parallel = true

[report]
precision = 2
show_missing = true
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __repr__

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError
